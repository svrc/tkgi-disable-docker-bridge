#!/bin/sh
set +e

sed -i 's/bridge=cni0/bridge=none/' /var/vcap/jobs/docker/bin/job_properties.sh
sed -i '/flannel/d' /var/vcap/jobs/docker/bin/job_properties.sh
sed -i '/DOCKER_MTU/d' /var/vcap/jobs/docker/bin/job_properties.sh
sed -i '/DOCKER_BRIDGE_NAME/d' /var/vcap/jobs/docker/bin/job_properties.sh
sed -i '/DOCKER_BRIDGE_CIDR/d' /var/vcap/jobs/docker/bin/job_properties.sh
sed -i '/create_network_bridge$/d'  /var/vcap/jobs/docker/bin/ctl
ip link del cni0
ip link del docker0
